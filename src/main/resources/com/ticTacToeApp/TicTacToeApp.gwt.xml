<module rename-to="TicTacToeApp">

    <!-- Inherit the core Web Toolkit stuff.                  -->
    <inherits name='com.google.gwt.user.User'/>

    <inherits name='com.google.gwt.uibinder.UiBinder'/>
    <inherits name="com.google.gwt.resources.Resources" />
    <inherits name="com.ticTacToeApp.TicTacToeApp"/>

    <!-- Specify the app entry point class.                   -->
    <entry-point class='com.ticTacToeApp.client.TicTacToeApp'/>


    <!-- Specify the app servlets.                   -->
    <servlet path='/TicTacToeAppService' class='com.ticTacToeApp.server.TicTacToeAppServiceImpl'/>

</module>